{
  "name": "kudos.bike",
  "version": "1.1.0",
  "description": "MBC 11ty website for Kudos TT series 2022",
  "main": "eleventy.config.js",
  "type": "module",
  "scripts": {
    "html:dev": "ELEVENTY_ENV=dev set DEBUG=Eleventy* & npx @11ty/eleventy --serve",
    "html:prd": "ELEVENTY_ENV=production NODE_ENV=production npx @11ty/eleventy",
    "css:dev": "postcss ./src/scss/style.scss --output ./_site/css/style.css --env development --verbose --watch",
    "css:prd": "postcss ./src/scss/style.scss --output ./_site/css/style.css --env production",
    "js:dev": "rollup --config --watch --strictDeprecations",
    "js:prd": "NODE_ENV=production rollup --config",
    "js:lint": "npx eslint src/**/*.js",
    "build": "NODE_ENV=production npm-run-all --serial css:prd js:prd html:prd",
    "start": "NODE_ENV=develop run-p html:dev js:dev css:dev",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest --collectCoverage"
  },
  "keywords": [],
  "author": "welt",
  "license": "CC-BY-2.0",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@netlify/plugin-sitemap": "^0.8.1",
    "csvtojson": "^2.0.10",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "netlify-plugin-html-validate": "^1.1.1",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "dotenv": "^16.4.7",
    "postcss": "^8.4.49",
    "postcss-advanced-variables": "^5.0.0",
    "postcss-assets": "^6.0.0",
    "postcss-cli": "^11.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-hash": "^3.0.0",
    "postcss-map-get": "^0.3.0",
    "postcss-nested": "^7.0.2",
    "postcss-scss": "^4.0.9",
    "postcss-sort-media-queries": "^5.2.0",
    "rollup": "^4.28.1",
    "rollup-plugin-output-manifest": "^2.0.0"
  }
}
